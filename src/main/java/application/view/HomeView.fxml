<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane focusTraversable="true" minHeight="500.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane fx:id="borderPane" stylesheets="@../../css/mainStyle.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <VBox id="top" BorderPane.alignment="CENTER">
               <children>
                  <HBox fx:id="hbox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <children>
                        <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="SOMETIMES">
                           <children>
                              <ImageView fx:id="logo" fitHeight="100.0" fitWidth="100.0" onMouseClicked="#goToCenterHome" pickOnBounds="true" preserveRatio="true" />
                           </children>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </HBox>
                        <HBox fx:id="hboxSearch" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="SOMETIMES" />
                        <HBox>
                           <children>
                              <MenuButton fx:id="userMenu" alignment="TOP_RIGHT" mnemonicParsing="false" styleClass="option" HBox.hgrow="SOMETIMES">
                                 <items>
                                    <MenuItem mnemonicParsing="false" onAction="#goToProfile" styleClass="item" text="Profilo" />
                                    <MenuItem mnemonicParsing="false" onAction="#goToOrderList" styleClass="item" text="Lista ordini" />
                                    <MenuItem mnemonicParsing="false" onAction="#goToCart" styleClass="item" text="Carrello" />
                                    <MenuItem mnemonicParsing="false" onAction="#goToFavorite" styleClass="item" text="Preferiti" />
                                    <MenuItem mnemonicParsing="false" onAction="#logout" styleClass="item" text="Logout" />
                                 </items>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </MenuButton>
                           </children>
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin>
                        </HBox>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="5.0">
                     <children>
                        <MenuButton mnemonicParsing="false" prefWidth="100.0" styleClass="menu" text="Cani">
                           <items>
                              <MenuItem mnemonicParsing="false" onAction="#clickDogFood" styleClass="item" text="Cibo" />
                              <MenuItem mnemonicParsing="false" onAction="#ClickDogHygiene" styleClass="item" text="Igiene" />
                              <MenuItem mnemonicParsing="false" onAction="#clickDogAccessories" styleClass="item" text="Accessori" />
                           </items>
                        </MenuButton>
                        <MenuButton mnemonicParsing="false" prefWidth="100.0" styleClass="menu" text="Gatti">
                           <items>
                              <MenuItem mnemonicParsing="false" onAction="#clickCatFood" styleClass="item" text="Cibo" />
                              <MenuItem mnemonicParsing="false" onAction="#clickCatHygienic" styleClass="item" text="Igiene" />
                              <MenuItem mnemonicParsing="false" onAction="#clickCatAccessories" styleClass="item" text="Accessori" />
                           </items>
                        </MenuButton>
                        <MenuButton mnemonicParsing="false" prefWidth="100.0" styleClass="menu" text="Pesci">
                           <items>
                              <MenuItem mnemonicParsing="false" onAction="#clickFishFood" styleClass="item" text="Cibo" />
                              <MenuItem mnemonicParsing="false" onAction="#clickFishAquariums" styleClass="item" text="Acquari" />
                           </items>
                        </MenuButton>
                        <MenuButton mnemonicParsing="false" prefWidth="100.0" styleClass="menu" text="Uccelli">
                           <items>
                              <MenuItem mnemonicParsing="false" onAction="#clickBirdFood" styleClass="item" text="Cibo" />
                              <MenuItem mnemonicParsing="false" onAction="#clickBirdAccessories" styleClass="item" text="Accessori" />
                           </items>
                        </MenuButton>
                        <MenuButton mnemonicParsing="false" prefWidth="100.0" styleClass="menu" text="Roditori">
                           <items>
                              <MenuItem mnemonicParsing="false" onAction="#clickRodentsFood" styleClass="item" text="Cibo" />
                              <MenuItem mnemonicParsing="false" onAction="#clickRodentAccessories" styleClass="item" text="Accessori" />
                           </items>
                        </MenuButton>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
