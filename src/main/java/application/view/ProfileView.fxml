<BorderPane id="top" onKeyReleased="#confirmOnKey" prefHeight="500.0" prefWidth="500.0" stylesheets="@../../css/mainStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
